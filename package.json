    "googleapis": "150.0.1",
    "ical-generator": "9.0.0",
    "ical.js": "2.1.0",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.16.0",
    "node-ical": "0.20.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
